{
    "compilerOptions": {
      "target": "es2016",
      "module": "commonjs",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "experimentalDecorators": true,
      "jsxImportSource": "preact",
      "isolatedModules": true
    },
    "display": "Recommended",
    "$schema": "https://json.schemastore.org/tsconfig"
  }
